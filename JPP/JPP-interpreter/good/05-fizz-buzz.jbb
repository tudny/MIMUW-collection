
fun fizzBuzzSingle(val n: new Integer): String {
    if n % 3 == 0 && n % 5 == 0 {
        return "fizzbuzz";
    } else if n % 3 == 0 {
        return "fizz";
    } else if n % 5 == 0 {
        return "buzz";
    } else {
        return toString(n);
    }
}

fun fizzBuzz(val n: new Integer) {
    for i = 1..n {
        writeStr(fizzBuzzSingle(i));
        writeStr("\n");
    }
}

fizzBuzz(100);

